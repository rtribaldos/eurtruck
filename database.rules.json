{
  "rules": {
    "users": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$uid": {
        ".read": "auth != null && auth.uid == $uid"
      }
    },
    "items": {
      ".read": "auth != null"
    },
    "viajes": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "ofertas": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
